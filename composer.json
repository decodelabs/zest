{
    "name": "decodelabs/zest",
    "description": "Vite front end dev environment integration",
    "type": "library",
    "keywords": [ "vite", "frontend" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.8",
        "decodelabs/clip": "^0.3.16",
        "decodelabs/coercion": "^0.3",
        "decodelabs/collections": "^0.11",
        "decodelabs/exceptional": "^0.5.3",
        "decodelabs/genesis": "^0.11",
        "decodelabs/iota": "^0.2",
        "decodelabs/lucid": "^0.5",
        "decodelabs/overpass": "^0.2.8",
        "decodelabs/terminus": "^0.11.1",
        "decodelabs/typify": "^0.3.2",
        "decodelabs/veneer": "^0.12.3"
    },
    "require-dev": {
        "ext-pcntl": "*",

        "decodelabs/phpstan-decodelabs": "^0.7",
        "decodelabs/harvest": "^0.4.2"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Zest\\": "src/Zest/",
            "DecodeLabs\\Harvest\\": "src/Harvest/"
        }
    },
    "bin": [
        "bin/zest"
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        }
    },
    "repositories": {
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
